generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.sqlite"
}

model Product {
  id          String  @id @default(cuid())
  name        String  
  slug        String  @unique
  price       Float
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
